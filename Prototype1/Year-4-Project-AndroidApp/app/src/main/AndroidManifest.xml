<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chris.year_4_project" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.chris.year_4_project.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chris.year_4_project.DisplayEvents"
            android:label="@string/title_activity_display_events" >
        </activity>
        <activity
            android:name="com.example.chris.year_4_project.SingleEventListItem"
            android:label="@string/title_activity_single_event_list_item" >
        </activity>
    </application>

</manifest>
